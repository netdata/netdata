  "restTools": {
    "posthog-query": {
      "description": "Run a HogQL query via PostHog Query API and return raw results.",
      "method": "POST",
      "url": "https://app.posthog.com/api/projects/${POSTHOG_QUERY_API_PROJECT}/query",
      "headers": {
        "content-type": "application/json",
        "Authorization": "Bearer ${POSTHOG_QUERY_API_KEY}"
      },
      "parametersSchema": {
        "type": "object",
        "properties": {
          "hogql": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "hogql"
        ],
        "additionalProperties": false
      },
      "bodyTemplate": {
        "query": {
          "kind": "HogQLQuery",
          "query": "${parameters.hogql}"
        }
      }
    },
    "ask-netdata": {
      "description": "Query Netdata docs via Ask Netdata (SSE) and return final answer.",
      "method": "POST",
      "url": "https://agent-events.netdata.cloud/ask-netdata/api/chat/stream",
      "headers": {
        "content-type": "application/json",
        "accept": "text/event-stream",
        "Referer": "https://learn.netdata.cloud/"
      },
      "parametersSchema": {
        "type": "object",
        "properties": {
          "question": {
            "type": "string",
            "minLength": 1
          }
        },
        "required": [
          "question"
        ],
        "additionalProperties": false
      },
      "bodyTemplate": {
        "messages": [
          {
            "role": "user",
            "content": "${parameters.question}"
          }
        ]
      },
      "streaming": {
        "mode": "json-stream",
        "linePrefix": "data:",
        "discriminatorField": "type",
        "doneValue": "done",
        "answerField": "answer",
        "tokenValue": "token",
        "tokenField": "content",
        "timeoutMs": 60000,
        "maxBytes": 1048576
      }
    }
  },
