{
  "providers": {
    "vllm": {
      "type": "openai",
      "apiKey": "vllm",
      "baseUrl": "http://nova:8083/v1",
      "openaiMode": "chat"
    },
    "openai": {
      "apiKey": "${OPENAI_API_KEY}",
      "baseUrl": "https://api.openai.com/v1",
      "headers": {},
      "custom": {},
      "mergeStrategy": "overlay"
    },
    "anthropic": {
      "apiKey": "${ANTHROPIC_API_KEY}",
      "baseUrl": "https://api.anthropic.com/v1",
      "headers": {},
      "custom": {},
      "mergeStrategy": "overlay"
    },
    "google": {
      "apiKey": "${GOOGLE_API_KEY}",
      "baseUrl": "https://generativelanguage.googleapis.com",
      "headers": {},
      "custom": {},
      "mergeStrategy": "overlay"
    },
    "openrouter": {
      "apiKey": "${OPENROUTER_API_KEY}",
      "custom": {
        "provider": {
          "allow_fallbacks": true,
          "data_collection": "deny",
          "sort": "throughput"
        }
      },
      "headers": {},
      "mergeStrategy": "overlay"
    },
    "ollama": {
      "baseUrl": "http://nova:11434/v1",
      "custom": {
        "stream": true,
        "providerOptions": {
          "ollama": {
            "think": true,
            "options": {
              "num_ctx": 65536,
              "num_predict": 4096,
              "top_k": 0,
              "min_p": 0,
              "repeat_last_n": 64,
              "repeat_penalty": 1.1,
              "stop": []
            }
          }
        }
      },
      "headers": {}
    }
  },
  "accounting": {
    "file": "${HOME}/ai-agent-accounting.jsonl"
  },
  "defaults": {
    "llmTimeout": 120000,
    "toolTimeout": 60000,
    "temperature": 0.7,
    "topP": 1.0,
    "stream": false,
    "parallelToolCalls": true,
    "maxToolTurns": 10,
    "mcpInitConcurrency": 10
  },
  "mcpServers": {
    "file-ops": {
      "type": "stdio",
      "command": "/usr/local/bin/file-mcp-server",
      "enabled": true
    },
    "env-test": {
      "type": "stdio",
      "command": "node",
      "args": [
        "codex/tools/env-test-server.mjs"
      ],
      "env": {
        "BRAVE_API_KEY": "${BRAVE_API_KEY}"
      },
      "enabled": true
    },
    "brave": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ],
      "env": {
        "BRAVE_API_KEY": "${BRAVE_API_KEY}"
      },
      "enabled": true
    },
    "jina": {
      "type": "sse",
      "url": "https://mcp.jina.ai/sse",
      "headers": {
        "Authorization": "Bearer ${JINA_API_KEY}"
      },
      "enabled": true
    },
    "netdata_costa": {
      "type": "websocket",
      "url": "ws://localhost:19999/mcp?api_key=b1eb964a-ff57-48f1-b87a-15b01702cdc7",
      "enabled": true
    },
    "netdata_production": {
      "type": "websocket",
      "url": "ws://10.20.1.126:19999/mcp?api_key=1ddcfc20-0f08-4562-a5e5-f016786f1641",
      "enabled": true
    },
    "netdata_demos": {
      "type": "websocket",
      "url": "ws://10.20.1.96:19999/mcp?api_key=edfa4484-c721-4973-8ff4-58adc360031d",
      "enabled": true
    },
    "slack": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "slack-mcp-server@latest",
        "--transport",
        "stdio"
      ],
      "env": {
        "SLACK_MCP_XOXC_TOKEN": "${SLACK_MCP_XOXC_TOKEN}",
        "SLACK_MCP_XOXD_TOKEN": "${SLACK_MCP_XOXD_TOKEN}",
        "SLACK_MCP_ADD_MESSAGE_TOOL": "${SLACK_MCP_ADD_MESSAGE_TOOL}"
      },
      "enabled": true
    },
    "freshdesk": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "freshdesk-mcp"
      ],
      "env": {
        "FRESHDESK_API_KEY": "${FRESHDESK_API_KEY}",
        "FRESHDESK_DOMAIN": "${FRESHDESK_DOMAIN}"
      },
      "enabled": true
    },
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      },
      "enabled": true
    },
    "context7": {
      "type": "http",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "${CONTEXT7_API_KEY}"
      },
      "enabled": true
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "playwright-mcp"
      ],
      "env": {},
      "enabled": true
    },
    "hubspot": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@hubspot/mcp-server"
      ],
      "env": {
        "PRIVATE_APP_ACCESS_TOKEN": "${PRIVATE_APP_ACCESS_TOKEN}"
      },
      "enabled": true
    },
    "fireflies": {
      "type": "http",
      "url": "https://api.fireflies.ai/mcp",
      "headers": {
        "Authorization": "Bearer ${FIREFLIES_API_KEY}"
      },
      "enabled": true
    },
    "bigquery": {
      "type": "stdio",
      "command": "/home/costa/.local/bin/toolbox",
      "args": [
        "--prebuilt",
        "bigquery",
        "--stdio"
      ],
      "env": {
        "BIGQUERY_PROJECT": "${BIGQUERY_PROJECT}"
      },
      "enabled": true
    },
    "posthog": {
      "type": "http",
      "url": "https://mcp.posthog.com/mcp",
      "headers": {
        "Authorization": "Bearer ${POSTHOG_API_KEY}"
      },
      "enabled": true
    },
    "fetcher": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "fetcher-mcp"]
    }
  }
}
