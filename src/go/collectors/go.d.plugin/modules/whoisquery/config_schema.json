{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "go.d/whoisquery job configuration schema.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "source": {
      "type": "string"
    },
    "timeout": {
      "type": [
        "string",
        "integer"
      ]
    },
    "days_until_expiration_warning": {
      "type": "integer"
    },
    "days_until_expiration_critical": {
      "type": "integer"
    }
  },
  "required": [
    "name",
    "source"
  ]
}
