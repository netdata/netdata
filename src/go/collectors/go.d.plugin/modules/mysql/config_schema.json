{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "go.d/mysql job configuration schema.",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "dsn": {
      "type": "string"
    },
    "my.cnf": {
      "type": "string"
    },
    "update_every": {
      "type": "integer"
    },
    "timeout": {
      "type": [
        "string",
        "integer"
      ]
    }
  },
  "required": [
    "name",
    "dsn"
  ]
}
